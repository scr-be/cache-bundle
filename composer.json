{
    "name"        : "scribe/cache-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://scr.be/cache-bundle",
    "description" : "Simple and extensible userland caching abstraction layer with built-in support for Memcached, DB, and filesystem handlers.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://symfony-cache-bundle.docs.scribe.tools/issues",
        "source" : "https://symfony-cache-bundle.docs.scribe.tools/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "scribe-cache-bundle",
        "symfony-cache-bundle",
        "cache-bundle",
        "cache",
        "memcached",
        "database",
        "filesystem",
        "bundle",
        "symfony",
        "symfony2",
        "symfony-framework",
        "scribe",
        "scr",
        "scribenet",
        "scribeinc",
        "scribe-software",
        "scribe-services"
    ],
    "authors"     : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                  : ">=5.6",
        "scribe/mantle-bundle" : "2.0.*@dev"
    },
    "require-dev" : {
        "ext-memcached"                         : "~2.2",
        "ext-igbinary"                          : "~1.2",
        "ext-tidy"                              : "~2.0",
        "symfony/symfony"                       : "~2.6",
        "sensio/generator-bundle"               : "~2.5",
        "doctrine/doctrine-fixtures-bundle"     : "2.2.*@dev",
        "phpunit/phpunit"                       : "~4.5",
        "scrutinizer/ocular"                    : "~1.1",
        "satooshi/php-coveralls"                : "dev-multipleroot",
        "mindscape/raygun4php"                  : "dev-master",
        "sami/sami"                             : "*@dev"
    },
    "suggest" : {
        "ext-memcached" : "Required to use Memcached caching service."
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\CacheBundle\\" : "src/"
        }
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/scribenet/php-coveralls.git"
        }
    ]
}
