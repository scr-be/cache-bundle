{
    "name"        : "scribe/cache-bundle",
    "description" : "Simple and extensible userland caching abstraction layer with built-in support for APUu and Memcached.",
    "keywords"    : ["symfony2", "cache", "apc", "apcu", "memcache", "memcached", "userland cache"],
    "type"        : "symfony-bundle",
    "homepage"    : "https://symfony-cache-bundle.docs.scribe.tools/",
    "license"     : "MIT",
    "authors"     : [
        {
          "name"  : "Rob Frawley 2nd",
          "email" : "rmf@scribe.software"
        }
    ],
    "support" : {
        "email"  : "open@scribe.tools",
        "issues" : "https://symfony-cache-bundle.docs.scribe.tools/issues",
        "source" : "https://symfony-cache-bundle.docs.scribe.tools/git"
    },
    "require" : {
        "php"                  : ">=5.6",
        "scribe/mantle-bundle" : "2.0.*@dev"
    },
    "require-dev" : {
        "ext-memcached"                   : "~2.2",
        "ext-igbinary"                    : "~1.2",
        "ext-tidy"                        : "~2.0",
        "symfony/symfony"                 : "~2.6",
        "phpunit/phpunit"                 : "~4.5",
        "allure-framework/allure-phpunit" : "~1.1",
        "scrutinizer/ocular"              : "~1.1",
        "satooshi/php-coveralls"          : "~0.6",
        "codeclimate/php-test-reporter"   : "~0.1.2",
        "phpdocumentor/phpdocumentor"     : "dev-master",
        "mayflower/php-codebrowser"       : "~1.1",
        "leaphub/phpcs-symfony2-standard" : "~2.0"
    },
    "suggest" : {
        "ext-memcached" : "Required to use Memcached caching service."
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\CacheBundle\\" : "src/"
        }
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
