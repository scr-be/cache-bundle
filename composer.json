{
    "name"        : "scribe/cache-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://scr.be/cache-bundle",
    "description" : "Simple and extensible userland caching abstraction layer with built-in support for Memcached, DB, and filesystem handlers.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://symfony-cache-bundle.docs.scribe.tools/issues",
        "source" : "https://symfony-cache-bundle.docs.scribe.tools/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "scribe-cache-bundle",
        "symfony-cache-bundle",
        "cache-bundle",
        "cache",
        "memcached",
        "database",
        "filesystem",
        "bundle",
        "symfony",
        "symfony2",
        "symfony-framework",
        "scribe",
        "scr",
        "scribenet",
        "scribeinc",
        "scribe-software",
        "scribe-services"
    ],
    "authors"     : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                                  : ">=5.6.4",
        "scribe/mantle-bundle"                 : "2.0.*@dev"
    },
    "require-dev" : {
        "ext-memcached"                        : "~2.2",
        "ext-igbinary"                         : "~1.2",
        "ext-tidy"                             : "~2.0",
        "ext-mysqli"                           : "*",
        "symfony/symfony"                      : "~2.7.0",
        "symfony/framework-bundle"             : "~2.7.0",
        "symfony/stopwatch"                    : "~2.7.0",
        "symfony/var-dumper"                   : "~2.7.0",
        "symfony/debug-bundle"                 : "~2.7.0",
        "symfony/phpunit-bridge"               : "~2.7.0",
        "phpunit/phpunit"                      : "~4.6",
        "sensio/generator-bundle"              : "~2.5",
        "doctrine/orm"                         : "~2.5",
        "doctrine/common"                      : "~2.5",
        "doctrine/dbal"                        : "~2.5",
        "doctrine/doctrine-bundle"             : "~1.4",
        "doctrine/doctrine-fixtures-bundle"    : "2.2.*@dev",
        "doctrine/data-fixtures"               : "1.0.*@dev",
        "doctrine/migrations"                  : "~1.0@dev",
        "doctrine/doctrine-migrations-bundle"  : "~1.0",
        "sensiolabs/security-checker"          : "~2.0",
        "scrutinizer/ocular"                   : "~1.1.1",
        "satooshi/php-coveralls"               : "dev-multipleroot",
        "chriskite/phactory"                   : "~0.4.3",
        "fzaninotto/faker"                     : "~1.5.0",
        "sami/sami"                            : "~3.0",
        "nikic/php-parser"                     : ">=1.0,<1.2",
        "pdepend/pdepend"                      : "~2.0"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\CacheBundle\\" : "src/"
        }
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/scribenet/php-coveralls.git"
        }
    ]
}
