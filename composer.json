{
    "name"        : "scribe/cache-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://wonka-bundle.oss.scr.be/home",
    "description" : "Extensible, run-time resolved cache chain handler, supporting any interface-compatible tagged service the ability to act as a provider. Native support for Memcached, database, and filesystem handlers.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://wonka-bundle.oss.scr.be/issues",
        "source" : "https://wonka-bundle.oss.scr.be/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "cache-bundle", "scribe-cache-bundle", "cache", "memcached", "database", "filesystem", "bundle", "symfony", "symfony2", "symfony-framework", "scribe", "scr", "scribenet", "scribeinc", "scribe-software",         "scribe-services"
    ],
    "authors"    : [
        {
            "name"     : "Rob Frawley 2nd",
            "email"    : "rmf@scr.be",
            "homepage" : "https://scr.be/rmf",
            "role"     : "Project Lead"
        }
    ],
    "require" : {
        "php"                          : ">=5.6.4,~5.6|~7.0",
        "ext-memcached"                : "~2.2",
        "scribe/wonka-library"         : "~0.1@dev",
        "scribe/wonka-bundle"          : "~0.1@dev",
        "symfony/dependency-injection" : "~2.7|~2.8@dev"
    },
    "require-dev" : {
        "symfony/symfony"                   : "~2.7|~2.8@dev",
        "symfony/monolog-bundle"            : "~2.7|~2.8@dev",
        "symfony/stopwatch"                 : "~2.7|~2.8@dev",
        "symfony/var-dumper"                : "~2.7|~2.8@dev",
        "symfony/debug-bundle"              : "~2.7|~2.8@dev",
        "symfony/phpunit-bridge"            : "~2.7|~2.8@dev",
        "phpunit/phpunit"                   : "~4.8",
        "sensiolabs/security-checker"       : "~3.0",
        "scrutinizer/ocular"                : "~1.1",
        "satooshi/php-coveralls"            : "~0.6",
        "chriskite/phactory"                : "~0.4",
        "fzaninotto/faker"                  : "~1.5",
        "sami/sami"                         : "~3.0",
        "nikic/php-parser"                  : ">=1.0,<1.2"
    },
    "conflict" : {
        "scribe/mantle-bundle" : "<2.1"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\CacheBundle\\" : "src/"
        }
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "2.1-dev"
        }
    }
}
