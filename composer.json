{
    "name"        : "scr-be/cache-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://scr.be/cache-bundle",
    "description" : "Extensible, run-time resolved cache chain handler, supporting any interface-compatible tagged service the ability to act as a provider. Native support for Memcached, database, and filesystem handlers.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://scr.be/cache-bundle/issues",
        "source" : "https://scr.be/cache-bundle/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "cache-bundle", "scribe-cache-bundle", "cache", "memcached", "database", "filesystem", "bundle", "symfony", "symfony2", "symfony-framework", "scribe", "scr", "scribenet", "scribeinc", "scribe-software",         "scribe-services"
    ],
    "authors"    : [
        {
            "name"     : "Rob Frawley 2nd",
            "email"    : "rmf@scr.be",
            "homepage" : "https://scr.be/rmf",
            "role"     : "Project Lead"
        }
    ],
    "require" : {
        "php"                          : ">=5.6.4,~5.6|~7.0",
        "scr-be/wonka-bundle"          : "~0.2",
        "symfony/dependency-injection" : "~2.8|~3.0"
    },
    "require-dev" : {
        "symfony/symfony"        : "~2.8|~3.0",
        "symfony/monolog-bundle" : "~2.8|~3.0",
        "symfony/stopwatch"      : "~2.8|~3.0",
        "symfony/var-dumper"     : "~2.8|~3.0",
        "symfony/debug-bundle"   : "~2.8|~3.0",
        "symfony/phpunit-bridge" : "~2.8|~3.0",
        "phpunit/phpunit"        : "~5.0",
        "codacy/coverage"        : "~1.0",
        "satooshi/php-coveralls" : "~0.7",
        "chriskite/phactory"     : "~0.4",
        "fzaninotto/faker"       : "~1.5"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\CacheBundle\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Scribe\\CacheBundle\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "2.2-dev"
        }
    }
}
